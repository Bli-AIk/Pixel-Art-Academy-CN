@import "{retronator:landsofillusions}/style/style.import"

.pixelartacademy-pixelboy-apps-admissionweek.app
  position relative
  // Place above the shortcuts table so that it's not visible when we overscroll in Safari.
  z-index 11
  background atariGray(0)
  height 100%
  overflow-y auto

  .content
  .content .pixelartacademy-pixelboy-apps-admissionweek-dayview
    position relative
    min-height 100%
    background-image url("/pixelartacademy/pixelboy/apps/admissionweek/library.png")
    background-size 229rem 60rem
    background-position center bottom
    background-repeat no-repeat
    background-color atariGray(0)
