@import "{retronator:landsofillusions}/style/style.import"

.sanfrancisco-apartment-studio-computer-browser
  background red

  .navigation-bar
    position relative
    height 12rem

    .url-input
      display block
      height 100%
      width 280rem
      color black

    .close-button
      display block
      position absolute
      width 40rem
      height 100%
      top 0
      right 0

  .webpage
    absolute-cover()
    top 12rem
    overflow-y scroll
    transform translateZ(0)

    iframe
      width 100%
      height 100%
